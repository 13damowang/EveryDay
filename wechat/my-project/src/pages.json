{
	"easycom": {
		"autoscan": true,
		"custom": {
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
		}
	},
	"pages": [
		{
			"path": "pages/home/index",
			"style": {}
		},
		{
			"path": "pages/cate/index",
			"style": {}
		},
		{
			"path": "pages/cart/index",
			"style": {}
		},
		{
			"path": "pages/my/index",
			"style": {}
		}
	],
	"subPackages": [
		{
			"root": "subpkg",
			"pages": [
				{
					"path": "shopDetail/index",
					"style": {
						"navigationBarTitleText": "shopDetail"
					}
				},
				{
					"path": "shopList/index",
					"style": {
						"navigationBarTitleText": "shopList"
					}
				},
				{
					"path": "search/index",
					"style": {
						"navigationBarTitleText": "search"
					}
				}
			]
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "white",
		"navigationBarTitleText": "测试商城",
		"navigationBarBackgroundColor": "#c00000",
		"backgroundColor": "#fff"
	},
	"tabBar": {
		"color": "#000",
		"selectedColor": "#c00000",
		"list": [
			{
				"pagePath": "pages/home/index",
				"text": "首页",
				"iconPath": "static/tabBar/home.png",
				"selectedIconPath": "static/tabBar/home-active.png"
			},
			{
				"pagePath": "pages/cate/index",
				"text": "分类",
				"iconPath": "static/tabBar/cate.png",
				"selectedIconPath": "static/tabBar/cate-active.png"
			},
			{
				"pagePath": "pages/cart/index",
				"text": "购物车",
				"iconPath": "static/tabBar/cart.png",
				"selectedIconPath": "static/tabBar/cart-active.png"
			},
			{
				"pagePath": "pages/my/index",
				"text": "我的",
				"iconPath": "static/tabBar/my.png",
				"selectedIconPath": "static/tabBar/my-active.png"
			}
		]
	}
}